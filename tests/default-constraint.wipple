[intrinsic] Number : type
[intrinsic] String : type
[intrinsic] List : element => type
[intrinsic] Set : element => type

Collect : element collection => trait (element -> collection)
instance (Collect element (List element))
instance (Collect element (Set element))

collect :: element -> output where (Collect element output) (output :: List _)

_ : collect "hi"
_ : (collect "hi" :: Set _)
