{
    "compilerOptions": {
        "target": "ES2020",
        "lib": ["ESNext"],
        "module": "esnext",
        "moduleResolution": "bundler",
        "forceConsistentCasingInFileNames": true,
        "erasableSyntaxOnly": true,
        "noEmit": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "inlineSources": true,
        "strict": true,
        "jsx": "preserve"
    }
}
